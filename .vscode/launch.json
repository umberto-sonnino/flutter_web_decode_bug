  "version": "0.2.0",
  "configurations": [
    {
      "name": "decode_web_images",
      "request": "launch",
      "type": "dart",
      "program": "lib/main.dart",
      "args": [
        "-dchrome",
        "--dart-define=BROWSER_IMAGE_DECODING_ENABLED=false",
        "--web-renderer canvaskit"
      ]
    },
    {
      "name": "decode_web_images (profile mode)",
      "request": "launch",
      "type": "dart",
      "flutterMode": "profile"
    },
    {
      "name": "decode_web_images (release mode)",
      "request": "launch",
      "type": "dart",
      "flutterMode": "release"
    }
  ]
}
